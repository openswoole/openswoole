{
    "name": "openswoole/example-rpc-service",
    "description": "Example OpenSwoole RPC project",
    "repositories": [
        {
            "name": "openswoole/core", 
            "type": "path", "url": "../../../core"
        },
        {
            "name": "openswoole/grpc",
            "type": "path",
            "url": "../.."
        }
    ],
    "require": {
        "openswoole/grpc": "dev-master",
        "google/protobuf": "^3.21",
        "openswoole/core": "dev-master"
    },
    "suggest": {
        "ext-openswoole": "*"
    },
    "autoload": {
        "psr-4": {
            "": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
