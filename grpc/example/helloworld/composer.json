{
    "name": "openswoole/example-rpc-service",
    "description": "Example OpenSwoole RPC project",
    "repositories": [
        {
            "name": "openswoole/core",
            "type": "path",
            "url": "../../../core"
        },
        {
            "name": "openswoole/grpc",
            "type": "path",
            "url": "../.."
        }
    ],
    "require": {
        "openswoole/grpc": "^22.0",
        "google/protobuf": "^3.21",
        "openswoole/core": "^22.0"
    },
    "suggest": {
        "ext-openswoole": "*"
    },
    "autoload": {
        "psr-4": {
            "": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
