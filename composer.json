{
    "name": "openswoole/openswoole",
    "authors": [
        {
            "name": "Open Swoole Group",
            "email": "hello@openswoole.com"
        },
        {
            "name": "Bruce Dou",
            "email": "doubaokun@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "psr/http-message": "^1.0"
    },
    "suggest": {
        "ext-openswoole": "*",
        "google/protobuf": "^3.19"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-sockets": "*",
        "friendsofphp/php-cs-fixer": "^3.6",
        "openswoole/ide-helper": "*",
        "php-http/psr7-integration-tests": "^1.1",
        "phpunit/phpunit": "^9.5",
        "symplify/monorepo-builder": "^11.1",
        "vimeo/psalm": "^4.6"
    },
    "autoload": {
        "files": [
            "core/src/Coroutine/functions.php"
        ],
        "psr-4": {
            "OpenSwoole\\Core\\": "core/src/",
            "OpenSwoole\\GRPC\\": "grpc/src/"
        }
    },
    "replace": {
        "openswoole/core": "self.version",
        "openswoole/example": "self.version",
        "openswoole/example-rpc-service": "self.version",
        "openswoole/grpc": "self.version"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
