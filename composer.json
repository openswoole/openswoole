{
    "name": "openswoole/openswoole",
    "authors": [
        {
            "name": "OpenSwoole Group",
            "email": "hello@openswoole.com"
        }
    ],
    "require": {
        "google/protobuf": "^3.21",
        "league/container": "4.x-dev",
        "php": ">=8.3",
        "psr/http-message": "^1.0",
        "psr/http-server-middleware": "^1.0.0",
        "slim/psr7": "dev-main",
        "slim/slim": "^4.0"
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-sockets": "*",
        "friendsofphp/php-cs-fixer": "^3.6",
        "openswoole/ide-helper": "*",
        "php-http/psr7-integration-tests": "^1.1",
        "phpunit/phpunit": "^10.5",
        "symplify/monorepo-builder": "11.1.17",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "files": [
            "core/src/Coroutine/functions.php"
        ],
        "psr-4": {
            "OpenSwoole\\Core\\": "core/src/",
            "OpenSwoole\\GRPC\\": "grpc/src/",
            "Openswoole\\Example\\": "example/src/"
        }
    },
    "replace": {
        "openswoole/core": "*",
        "openswoole/example": "*",
        "openswoole/example-rpc-service": "*",
        "openswoole/grpc": "*",
        "openswoole/ide-helper": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
